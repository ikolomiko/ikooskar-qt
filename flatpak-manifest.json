{
    "id": "tr.com.ikooskar.ikoOSKAR",
    "branch": "main",
    "runtime": "org.kde.Platform",
    "runtime-version": "6.6",
    "sdk": "org.kde.Sdk",
    "command": "ikoOSKAR",
    "finish-args": [
        "--share=ipc",
        "--share=network",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri",
        "--filesystem=xdg-documents/ikoOSKAR:create"
    ],
    "cleanup": [
        "/include",
        "/lib",
        "*.la",
        "*.a"
    ],
    "modules": [
        {
            "name": "ikoOSKAR",
            "buildsystem": "cmake-ninja",
            "sources": [
                {
                    "type": "dir",
                    "path": "."
                }
            ]
        }
    ]
}

